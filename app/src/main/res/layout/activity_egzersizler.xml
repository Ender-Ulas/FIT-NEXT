<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#47D1C5"
    tools:context=".Egzersizler">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="EGZERSİZLER"
        android:textColor="#217F8C"
        android:textSize="55sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_sirt"
        android:layout_width="269dp"
        android:layout_height="73dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#217F8C"
        android:text="SIRT"
        android:textColor="#0C4D91"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_gogus"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="202dp"
        android:layout_height="121dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.004"
        app:srcCompat="@drawable/egzersizicon" />

    <Button
        android:id="@+id/btn_omuz"
        android:layout_width="269dp"
        android:layout_height="73dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#217F8C"
        android:text="OMUZ"
        android:textColor="#0C4D91"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_karinkasi"
        android:layout_width="269dp"
        android:layout_height="73dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#217F8C"
        android:text="KARIN KASI"
        android:textColor="#0C4D91"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_sirt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btn_gogus"
        android:layout_width="269dp"
        android:layout_height="73dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#217F8C"
        android:text="GÖĞÜS"
        android:textColor="#0C4D91"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_omuz"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="96dp"
        android:layout_height="72dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_karinkasi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_karinkasi"
        app:srcCompat="@drawable/kaskarn" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="91dp"
        android:layout_height="72dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_sirt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_sirt"
        app:srcCompat="@drawable/sirt" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="91dp"
        android:layout_height="80dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_gogus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_gogus"
        app:srcCompat="@drawable/gogus" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="94dp"
        android:layout_height="73dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_omuz"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/omuz" />

</androidx.constraintlayout.widget.ConstraintLayout>